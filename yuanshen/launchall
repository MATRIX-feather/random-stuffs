#!/bin/bash
#cd进脚本的目录
source "$(dirname "$(readlink -f "$0")")/common/common"

for rd in ${PWD}/*;do
    if [ -d "$rd" ];then
        #log "D: $rd"
        d="$rd"

        for f in "$d"/start*;do
            if [ "$f" == "$d/start*" ];then
                #log "SKIP:$f"
                continue
            fi

            #log "F: $f"
            log "正在启动$f"
            "$f" & disown;
        done;
    fi;
done;